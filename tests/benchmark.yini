[#define]
app_name = "YINI Benchmark"
version = "1.0.0"
is_enabled = true
base_path = Path(C:/ProgramData/YiniApp)
default_user = "benchmark_user"

// Base component definitions
[Component.Position]
x = 0.0
y = 0.0
z = 0.0

[Component.Render]

// Simple YINI Configuration Example

[Server]
host = "localhost"
port = 8080
max_connections = 100
timeout = 30.0
debug_mode = true

[Database]
connection_string = "mongodb://localhost:27017"
pool_size = 10
retry_attempts = 3

[Logging]
level = "INFO"
file = "app.log"
max_size = 10485760  // 10MB in bytes
rotation = true

{
    "name": "yini",
    "displayName": "YINI Language Support",
    "description": "Syntax highlighting for the YINI language.",
    "version": "0.0.2",
    "engines": {
        "vscode": "^1.82.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "main": "./src/extension.js",
    "activationEvents": [
        "onLanguage:yini"
    ],
    "contributes": {
        "languages": [{
            "id": "yini",
            "aliases": ["YINI", "yini"],
            "extensions": [".yini"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "yini",
            "scopeName": "source.yini",
            "path": "./syntaxes/yini.tmLanguage.json"
        }]
    }
}
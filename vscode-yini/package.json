{
  "name": "yini-language-support",
  "displayName": "YINI Language Support",
  "description": "Language support for the YINI configuration file format.",
  "version": "0.0.1",
  "publisher": "yini-project",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:yini"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "yini",
        "aliases": [
          "YINI",
          "yini"
        ],
        "extensions": [
          ".yini",
          ".YINI"
        ],
        "configuration": "./language-configuration.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "vscode-languageclient": "^8.0.2"
  }
}